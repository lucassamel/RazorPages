@page "/movies"
@model RazorPages.Pages.Movies.IndexModel
@{
    ViewData["Title"] = "My Movies";
}


<h1>Movies</h1>
<ul class="list-group">
    @foreach (var movie in Model.Movies)
    {
        <li class="list-group-item">
            <a asp-page="/Movies/Details" asp-route-permalink="@movie.Permalink" >@movie.Name</a>
        </li>
    }
</ul>


